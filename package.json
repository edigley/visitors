{
  "name": "visitors",
  "repository": {
    "type": "git",
    "url": "https://github.com/edigley/visitors.git"
  },
  "version": "0.0.1",
  "description": "Visitor Access Control",
  "main": "index.js",
  "scripts": {
    "prepush": "npm run eslint -- --quiet && npm run test",
    "deploy": "webpack --package --deploy",
    "package": "webpack --package",
    "cpd": "jscpd --path=app/ --skip-comments",
    "eslint": "eslint app/** --fix",
    "jsonServer": "json-server --watch api/application_db.json --port 9999 --routes api/routes.json",
    "start": "http-server --port 8080 ./dist -s | webpack --watch -d --profile --progress --env=local --run",
    "start:dev": "http-server ./dist -s | webpack --watch -d --profile --progress --env=dev --run",
    "start:inline": "http-server ./dist -s | webpack-dev-server --env=dev --inline --port 8081 --run",
    "start:prod": "http-server ./dist | webpack --env=prod --profile --run",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register -r jsdom-global/register test/**/Input_test.js",
    "cover": "nyc --reporter=lcov --reporter=text node_modules/mocha/bin/_mocha test/**/Input_test.js --require ./test/config/mocha-setup-local --compilers js:babel-register,css:css-null-compiler.js"
  },
  "keywords": [
    "react",
    "redux",
    "esling",
    "babel"
  ],
  "author": "Edigley Fraga",
  "license": "GPL-3.0",
  "dependencies": {
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.5.2",
    "redux-form": "^6.6.3",
    "react-bootstrap": "^0.30.9",
    "axios": "^0.13.1",
    "lodash": "^4.16.3",
	"prop-types": "^15.5.8",
    "react-virtualized": "^7.22.1",
    "react-addons-shallow-compare": "^15.3.0",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.1",
    "redux-immutable-state-invariant": "^1.2.4",
    "react-redux-i18n": "^1.8.0",
    "react-datepicker": "^0.29.0",
    "css-loader": "^0.24.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.14",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "enzyme": "^2.8.2",
    "es6-promise": "^3.2.1",
    "eslint-loader": "1.4.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-react": "^1.1.0",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "json-loader": "0.5.4",
    "mocha": "^3.2.0",
    "postcss-loader": "0.9.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-uglify-js-plugin": "^1.1.9",
    "yargs": "^6.5.0",
    "zip-webpack-plugin": "^0.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
